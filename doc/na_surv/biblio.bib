pages = {10506--11},
pmid = {17563357},
title = {{The effect of geographic range on extinction risk during background and mass extinction.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1890565\&tool=pmcentrez\&rendertype=abstract},
volume = {104},
