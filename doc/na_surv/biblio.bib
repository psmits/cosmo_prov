month = jul,
number = {27},
pages = {E2460--9},
pmid = {23781101},
title = {{Model for macroevolutionary dynamics.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3703965\&tool=pmcentrez\&rendertype=abstract},
volume = {110},
year = {2013}
}
