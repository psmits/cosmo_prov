issn = {1558-5646},
journal = {Evolution; international journal of organic evolution},
keywords = {Animals,Anthozoa,Anthozoa: anatomy \& histology,Anthozoa: genetics,Bayes Theorem,Biological Evolution,Computational Biology,DNA, Mitochondrial,DNA, Mitochondrial: genetics,Evolution, Molecular,Extinction, Biological,Fossils,Genetic Speciation,Models, Genetic,Phylogeny},
month = nov,
