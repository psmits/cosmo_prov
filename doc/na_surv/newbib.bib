keywords = {Animals,Biological Evolution,Ecosystem,Fossils,Herbivory,Herbivory: physiology,Logistic Models,Mammals,Mammals: anatomy \& histology,Mammals: genetics,Mammals: physiology,Rain,Temperature,Time Factors,Tooth,Tooth: anatomy \& histology},
month = jul,
number = {1739},
pages = {2793--9},
pmid = {22456884},
title = {{Dental functional traits of mammals resolve productivity in terrestrial ecosystems past and present.}},
