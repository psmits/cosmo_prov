keywords = {Bayes Theorem,Bias (Epidemiology),Data Interpretation, Statistical,Epidemiology,Epidemiology: statistics \& numerical data,Models, Statistical,Models, Theoretical,Risk Factors},
month = sep,
number = {6},
pages = {430--42},
pmid = {8213748},
title = {{A Bayesian approach to measurement error problems in epidemiology using conditional independence models.}},
volume = {138},
year = {1993}
}
